{
  "name": "projectthree",
  "version": "1.0.0",
  "description": "MERN",
  "main": "server.js",
  "private": true,
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "devStart": "nodemon server.js",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlovejo2/Consilience_Learning_PlatForm.git"
  },
  "author": "James",
  "license": "",
  "bugs": {
    "url": "https://github.com/jlovejo2/Consilience_Learning_PlatForm"
  },
  "homepage": "https://github.com/DopamineDriven/ProjectThree#readme",
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "@date-io/date-fns": "^2.5.0",
    "@material-ui/core": "^4.9.9",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chance": "^1.1.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.5",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.5",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "socket.io": "^2.3.0",
    "validator": "^13.0.0"
  }
}
